{
  "name": "mailxpert/oauth2-mailxpert",
  "description": "mailXpert OAuth 2.0 Client Provider for the PHP League OAuth 2.0 Client",
  "license": "MIT",
  "authors": [
    {
      "name": "ARTACK WebLab GmbH",
      "homepage": "https://artack.ch"
    }
  ],
  "keywords": [
    "php",
    "api",
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authentication"
  ],
  "require": {
    "php": "^7.4|^8.0",
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/var-dumper": "^5.4",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Mailxpert\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mailxpert\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4.33"
    }
  }
}
